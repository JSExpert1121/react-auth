      "!<rootDir>/node_modules/",
      "!src/store/config.ts"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
